	</parameter>
	<queryString language="SQL">
		<![CDATA[select mat.serie_periodo as serie, 
			mat.curso as curso, mat.turno as turno,
 			a.nome as aluno_nome, 
 			a.data_nascimento as aluno_data_nascimento,
 			a.naturalidade as aluno_naturalidade,
 			a.sexo as sexo,
 			a.cor as cor,
 			a.endereco as aluno_endereco,
 			a.bairro as aluno_bairro,
 			a.cep as aluno_cep,
 			p.nome as pai_nome,
 			p.data_nascimento as pai_nascimento,
 			p.cpf as pai_cpf,
 			p.rg as pai_rg,
 			p.orgao_expedidor as pai_expedidor,
 			p.data_expedicao as pai_expedicao,
 			p.grau_instrucao as pai_instrucao,
 			p.email as pai_email,
 			p.celular as pai_celular,
 			p.local_trabalho as pai_trabalho,
 			p.profissao as pai_profissao,
 			p.telefone_trabalho as pai_tel_trabalho,
 			m.nome as mae_nome,
 			m.data_nascimento as mae_nascimento,
 			m.cpf as mae_cpf,
 			m.rg as mae_rg,
 			m.orgao_expedidor as mae_expedidor,
 			m.data_expedicao as mae_expedicao,
 			m.grau_instrucao as mae_instrucao,
 			m.email as mae_email,
 			m.celular as mae_celular,
 			m.local_trabalho as mae_trabalho,
 			m.profissao as mae_profissao,
 			m.telefone_trabalho as mae_tel_trabalho,
 			par1.nome as par_1_nome,
 			par1.celular as par_1_celular,
			par2.nome as par_2_nome,
 			par2.celular as par_2_celular,
 			s.esclarecimento as esclarecimento,
 			s.quem_cuida as quem_cuida,
 			s.religiao as religiao,
 			s.n_irmaos as n_irmaos,
 			s.posicao_familia as posicao_familia,	
			s.outras_informacoes as situacoes
from matriculas mat
inner join alunos a
on a.id = mat.aluno_id
left join pais p
on p.id = a.pai_id
inner join maes m
on m.id = a.mae_id
left join parentes par1
on par1.id = a.parente_1_id
left join parentes par2
on par2.id = a.parente_2_id
inner join situacoes s
on s.id = a.situacoes_id
where a.id = $P{DQ_UserInput}]]>
	</queryString>
